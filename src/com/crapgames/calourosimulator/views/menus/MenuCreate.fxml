<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.collections.*?>
<?import javafx.scene.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.collections.FXCollections?>
<?import javafx.scene.control.*?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import java.lang.*?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.72" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.crapgames.calourosimulator.views.menus.MenuCreate">
    <children>
        <ImageView fitHeight="635.0" fitWidth="802.0" pickOnBounds="true">
            <image>
                <Image url="@../../../../../assets/backgrounds/ru.png" />
            </image>
            <cursor>
                <Cursor fx:constant="TEXT" />
            </cursor>
        </ImageView>
        <TextField fx:id="username" layoutX="401.0" layoutY="234.0" promptText="Nome do Pernonagem" />
        <Button fx:id="createStuff" layoutX="401.0" layoutY="366.0" mnemonicParsing="false" onAction="#writeBuffers" text="Criar e Iniciar" />
        <Button fx:id="quitButton" layoutX="401.0" layoutY="326.0" mnemonicParsing="false" onAction="#quitButtonAction" text="Sair" />
        <ComboBox fx:id="sexBox" layoutX="401.0" layoutY="280.0" onAction="#chooseSexButton" prefWidth="150.0" promptText="Sexo">
            <items>
                <FXCollections fx:factory="observableArrayList">
                    <String fx:value="Masculino" />
                    <String fx:value="Feminino" />
                    <String fx:value="Transexual" />
                    <String fx:value="Berg" />
                    <String fx:value="Todas as opções" />
                </FXCollections>
            </items>
        </ComboBox>
      <Label layoutX="401.0" layoutY="171.0" text="Criar Personagem" textFill="#000000f0">
         <font>
            <Font size="29.0" />
         </font>
      </Label>
      <Button fx:id="goBack" layoutX="515.0" layoutY="326.0" mnemonicParsing="false" onAction="#mainButton" text="Voltar" />
    </children>
</AnchorPane>
